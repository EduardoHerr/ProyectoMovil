<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:minWidth="25px"
    android:minHeight="25px"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/linearLayout1"
    android:gravity="center"
    android:background="@mipmap/wallmovil">
    <LinearLayout
        android:orientation="vertical"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="369.5dp"
        android:layout_height="477.5dp"
        android:id="@+id/linearLayout2"
        android:background="@drawable/xmlredondeo">
        <LinearLayout
            android:orientation="horizontal"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:id="@+id/linearLayout3"
            android:layout_marginTop="15.5dp">
            <EditText
                android:inputType="textMultiLine"
                android:layout_width="213.0dp"
                android:layout_height="match_parent"
                android:id="@+id/txtBuscarcat"
                android:background="@drawable/busquedatext"
                android:layout_marginTop="2.0dp"
                android:layout_marginLeft="10.5dp" />
            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:id="@+id/btnbuscarcat"
                android:background="@mipmap/lupa"
                android:layout_marginLeft="8.5dp" />
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:id="@+id/linearLayout4"
            android:layout_marginLeft="10.0dp"
            android:layout_marginRight="7.0dp">
            <TextView
                android:text="TIPO"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView1"
                android:textAlignment="center" />
            <EditText
                android:inputType="textMultiLine"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/txttipocat" />
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:id="@+id/linearLayout5"
            android:layout_marginLeft="12.0dp"
            android:layout_marginRight="13.5dp">
            <TextView
                android:text="Descripcion"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView2" />
            <EditText
                android:inputType="textMultiLine"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/txtdesccat" />
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/linearLayout10"
            android:layout_marginBottom="27.5dp"
            android:layout_marginTop="0.0dp">
            <Button
                android:text="REGISTRAR"
                android:layout_width="110.0dp"
                android:layout_height="wrap_content"
                android:id="@+id/btnRegistrarcat"
                android:layout_marginLeft="10dp" />
            <Button
                android:text="MODIFICAR"
                android:layout_width="110.0dp"
                android:layout_height="49.5dp"
                android:id="@+id/btnModificarcat"
                android:layout_marginLeft="10dp" />
            <Button
                android:text="ELIMINAR"
                android:layout_width="110dp"
                android:layout_height="wrap_content"
                android:id="@+id/btnEliminarcat"
                android:layout_marginLeft="10dp" />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
