<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:minWidth="25px"
    android:minHeight="25px"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/linearLayout1"
    android:gravity="center"
    android:background="@mipmap/wallmovil">

    
        <LinearLayout
            android:orientation="vertical"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="354.0dp"
            android:layout_height="503.0dp"
            android:id="@+id/linearLayout2"
            android:background="@drawable/xmlredondeo">
            <LinearLayout
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="330.0dp"
                android:layout_height="60dp"
                android:id="@+id/linearLayout3">
                <EditText
                    android:inputType="textMultiLine"
                    android:layout_width="213.0dp"
                    android:layout_height="match_parent"
                    android:id="@+id/txtBuscar"
                    android:background="@drawable/busquedatext"
                    android:layout_marginRight="14.5dp"
                    android:textAlignment="center"
                    android:textStyle="bold"
                    android:layout_marginTop="8.0dp"
                    android:layout_marginLeft="10.0dp" />
                <ImageButton
                    android:layout_width="65.0dp"
                    android:layout_height="53.5dp"
                    android:id="@+id/imageButton1"
                    android:background="@mipmap/lupa"
                    android:layout_marginTop="8.0dp" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="340dp"
                android:layout_height="60dp"
                android:id="@+id/linearLayout4"
                android:layout_marginLeft="8.0dp">
            <TextView
                android:text="NOMBRE:"
                android:layout_width="71.5dp"
                android:layout_height="wrap_content"
                android:id="@+id/textView1"
                android:textAlignment="center"
                android:textColor="@android:color/background_dark" />
            <EditText
                android:inputType="textMultiLine"
                android:layout_width="262.0dp"
                android:layout_height="match_parent"
                android:id="@+id/txtNombre"
                android:textColor="@android:color/background_dark" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="340dp"
                android:layout_height="60dp"
                android:id="@+id/linearLayout5"
                android:layout_marginLeft="8dp">
            <TextView
                android:text="APELLIDO:"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView2"
                android:textColor="@android:color/background_dark" />
            <EditText
                android:inputType="textMultiLine"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/txtApellido"
                android:textColor="@android:color/black" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="340dp"
                android:layout_height="60dp"
                android:id="@+id/linearLayout6"
                android:layout_marginLeft="8dp">
            <TextView
                android:text="CEDULA:"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView3"
                android:textColor="@android:color/black" />
            <EditText
                android:inputType="textMultiLine"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/txtCedula"
                android:textColor="@android:color/black" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="340dp"
                android:layout_height="60dp"
                android:id="@+id/linearLayout7"
                android:layout_marginLeft="8dp">
            <TextView
                android:text="CORREO:"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView4"
                android:textColor="@android:color/black" />
            <EditText
                android:inputType="textMultiLine"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/txtCorreo"
                android:textColor="@android:color/black" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="340dp"
                android:layout_height="60dp"
                android:id="@+id/linearLayout8"
                android:layout_marginLeft="8dp">
            <TextView
                android:text="CLAVE:"
                android:layout_width="57.0dp"
                android:layout_height="wrap_content"
                android:id="@+id/textView5"
                android:textColor="@android:color/black" />
            <EditText
                android:inputType="textMultiLine"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/txtPass"
                android:layout_marginRight="0.0dp"
                android:textColor="@android:color/black" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:id="@+id/linearLayout9"
                android:layout_marginLeft="8dp">
            <TextView
                android:text="ROL:"
                android:layout_width="52.0dp"
                android:layout_height="wrap_content"
                android:id="@+id/textView6"
                android:layout_gravity="center_vertical"
                android:textColor="@android:color/black" />
                <Spinner
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:id="@+id/spinner1" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="350.0dp"
                android:layout_height="58.0dp"
                android:id="@+id/linearLayout10"
                android:layout_marginTop="17.5dp">
            <Button
                android:text="REGISTRAR"
                android:layout_width="104.0dp"
                android:layout_height="match_parent"
                android:id="@+id/btnRegistrar"
                android:layout_marginLeft="10dp"
                android:background="#ff005aff"
                android:textColor="@android:color/background_light"
                android:textSize="13dp" />
            <Button
                android:text="MODIFICAR"
                android:layout_width="104.5dp"
                android:layout_height="match_parent"
                android:id="@+id/btnModificar"
                android:layout_marginLeft="10dp"
                android:background="#ff068a1b"
                android:textColor="@android:color/background_light"
                android:textSize="13dp" />
            <Button
                android:text="ELIMINAR"
                android:layout_width="104.5dp"
                android:layout_height="match_parent"
                android:id="@+id/btnEliminar"
                android:layout_marginLeft="10dp"
                android:background="#ffc30505"
                android:textColor="@android:color/background_light" />
            </LinearLayout>
        </LinearLayout>
</LinearLayout>